{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": ["get"],
      "route": "weather/{location?}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ],
  "cors": {
    "allowedOrigins": ["http://localhost:3000"],
    "allowedMethods": ["GET", "OPTIONS"]
  }
}